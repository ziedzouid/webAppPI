<head>

    <!-- Basic Page Needs -->
    <meta charset="utf-8">
    <title>My ride</title>
    <meta name="description" content="">
    <meta name="author" content="">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <!-- Mobile Specific Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- Styles -->

    <!-- Bootstrap -->
    <link href="{{ asset ('templates/css/bootstrap.min.css') }}" rel="stylesheet">
    <!-- Forms -->
    <link href="{{ asset ('templates/css/jquery.idealforms.css') }}" rel="stylesheet">
    <!-- Select  -->
    <link href="{{ asset ('templates/css/jquery.idealselect.css') }}" rel="stylesheet">
    <!-- Slicknav  -->
    <link href="{{ asset ('templates/css/slicknav.css') }}" rel="stylesheet">
    <!-- Main style -->
    <link href="{{ asset ('templates/css/style.css') }}" rel="stylesheet">


    <!-- Modernizr -->
    <script src="{{ asset ('templates/js/modernizr.js') }}"></script>

    <!-- Fonts -->
    <link href="{{ asset ('templates/css/font-awesome.min.css') }}" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Montserrat' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,300' type='text/css'>
    <link href='http://netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css' type='text/css'>

    <link href="{{ asset ('templates/css/feed.css') }}" rel="stylesheet">
    {% block stylesheet %}
    {% endblock %}

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>

    <![endif]-->

</head>

<body>

<header class="header">

    <div class="top-menu">

        <section class="container">
            <div class="row">

                <div class="col-md-4 col-sm-4 col-xs-12">
                    <div class="user-log">


                        {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                            <a href="#">{{ 'layout.logged_in_as'|trans({'%username%': app.user.username}, 'FOSUserBundle') }}</a>
                            <a href="{{ path('fos_user_security_logout') }}"> /SIGNOUT</a>
                            <a href="{{ path('dashboard_client') }}"> /MY ACOUNT</a>



                        {% else %}
                            <a data-toggle="modal" data-target="#loginModal">
                                Log in
                            </a>/
                            <a data-toggle="modal" data-target="#regModal">
                                Sign up
                            </a>
                        {% endif %}


                    </div><!-- end .user-log -->
                </div><!-- end .col-sm-4 -->

                <div class="col-md-8 col-sm-8 col-xs-12">

                    <ul class="social-icons">

                        <li>
                            <a class="facebook" href="#">
                                <i class="fa fa-facebook"></i>
                            </a>
                        </li>
                        <li>
                            <a class="twitter" href="#">
                                <i class="fa fa-twitter"></i>
                            </a>
                        </li>
                        <li>
                            <a class="google" href="#">
                                <i class="fa fa-google-plus"></i>
                            </a>
                        </li>
                    </ul>

                </div><!-- end .col-sm-8 -->

            </div><!-- end .row -->
        </section><!-- end .container -->

    </div><!-- end .top-menu -->

    <div class="main-baner">

        <div class="fullscreen background parallax clearfix"
             style="background-image:url('{{ asset ('templates/img/tumblr_n7yhhvUQtx1st5lhmo1_1280.jpg') }}');"
             data-img-width="1600" data-img-height="1064">

            <div class="main-parallax-content">

                <div class="second-parallax-content">

                    <section class="container">

                        <div class="row">

                            <div class="main-header-container clearfix">

                                <div class="col-md-4 col-sm-12 col-xs-12">

                                    <div class="logo">
                                        <h1>CoolVoiturage</h1>
                                    </div><!-- end .logo -->



                                </div><!-- end .col-sm-4 -->

                                {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}


                                    {% block nav %}
                                        <div class="col-md-8 col-sm-8 col-xs-12">

                                            <nav id="nav" class="main-navigation">

                                                <ul class="navigation">
                                                    <li>
                                                        <a href="#">Home</a>
                                                    </li>
                                                    <li>
                                                        <a href="#">rides</a>
                                                    </li>
                                                    <li>
                                                        <a href="#">Submit</a>
                                                    </li>
                                                    <li>
                                                        <a href="#">Pages</a>
                                                        <ul class="sub-menu">
                                                            <li>
                                                                <a href="#">Events</a>
                                                            </li>
                                                            <li>
                                                                <a href="#">Single
                                                                    post</a>
                                                            </li>
                                                            <li>
                                                                <a href="#">Single
                                                                    article</a>
                                                            </li>
                                                        </ul>
                                                    </li>
                                                    <li>
                                                        <a href="{{ path('actualite_index') }}">Blog</a>
                                                    </li>
                                                    <li>
                                                        <a href="{{ path('feedback_newfeed') }}">Feedback</a>
                                                    </li>
                                                    <li>
                                                        <a href="{{ path('fos_message_inbox') }}">Chat</a>
                                                    </li>
                                                </ul>

                                            </nav><!-- end .main-navigation -->
                                        </div><!-- end .col-md-8 -->
                                    {% endblock %}
                                {% endif %}

                            </div><!-- end .main-header-container -->

                        </div><!-- end .row -->

                    </section><!-- end .container -->

                </div><!-- end .second-parallax-content -->

            </div><!-- end .main-parallax-content -->

        </div><!-- end .background .parallax -->

    </div><!-- end .main-baner -->

</header><!-- end .header -->
<div>
    {% block body_content %}

    {% endblock %}
</div>
<footer id="footer">

    <div class="footer-copyright">

        <div class="container">
            <div class="row">

                <div class="col-md-12 col-sm-12 col-xs-12">
                    Copyright by My ride
                </div>

            </div><!-- end .row -->
        </div><!-- end .container -->

    </div><!-- end .footer-copyright -->

</footer><!-- end #footer -->


{% block fos_user_content %}
    {{ render(controller('FOSUserBundle:Security:login')) }}
{% endblock fos_user_content %}

{% block registration_content %}
    {{ render(controller('FOSUserBundle:Registration:register')) }}
{% endblock registration_content %}

<!-- Javascript -->
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
<!-- Bootstrap -->
<script type="text/javascript" src="{{ asset ('templates/js/bootstrap.min.js') }}"></script>
<!-- Main jQuery -->
<script type="text/javascript" src="{{ asset ('templates/js/jquery.main.js') }}"></script>
<!-- Form -->
<script type="text/javascript" src="{{ asset ('templates/js/jquery.idealforms.min.js') }}"></script>
<script type="text/javascript" src="{{ asset ('templates/js/jquery.idealselect.min.js') }}"></script>
<script type="text/javascript" src="{{ asset ('templates/js/jquery-ui-1.10.4.custom.min.js') }}"></script>
<!-- Menu -->
<script type="text/javascript" src="{{ asset ('templates/js/hoverIntent.js') }}"></script>
<script type="text/javascript" src="{{ asset ('templates/js/superfish.js') }}"></script>
<!-- Counter-Up  -->
<script type="text/javascript" src="http://cdnjs.cloudflare.com/ajax/libs/waypoints/2.0.3/waypoints.min.js"></script>
<script type="text/javascript" src="{{ asset ('templates/js/jquery.counterup.min.js') }}"></script>
<!-- Rating  -->
<script type="text/javascript" src="{{ asset ('templates/js/bootstrap-rating-input.min.js') }}"></script>
<!-- Slicknav  -->
<script type="text/javascript" src="{{ asset ('templates/js/jquery.slicknav.min.js') }}"></script>

</body>
</html>